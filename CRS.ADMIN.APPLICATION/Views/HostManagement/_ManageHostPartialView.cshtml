                                   {
                                       @class = "w-full",
                                       @placeholder = CRS.ADMIN.APPLICATION.Resources.Resource.Host_Name,
                                       //@onkeypress = "return isAlphabetWithSpace(event)",
                                       autocomplete = "off",
                                       @maxLength = "100"
                                   })
