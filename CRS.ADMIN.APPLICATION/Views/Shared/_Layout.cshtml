        .border-card-balance {
            display: flex; /* Use flexbox for layout */
            align-items: center; /* Center items vertically */
            width: auto; /* Allow width to adjust based on content */
            height: 40px;
            position: absolute; /* Ensure positioning is respected */
            top: 20px;
            left: 1000px;
            padding: 6px 12px;
            gap: 8px;
            border-radius: 4px;
            border: 1px solid #FCE6EF;
            opacity: 1;
            background-color: #FCE6EF;
        }

            .border-card-balance img {
                height: 100%; /* Adjust the height of the image to fit the container */
                width: auto; /* Maintain aspect ratio of the image */
            }

        .card-text {
            display: flex; /* Use flexbox to align text content */
            flex-direction: column; /* Stack text vertically */
            gap: 0; /* Remove gap between text elements */
            color: #d75a8b;
            flex: 1; /* Allow the text container to take up available space */
            line-height: 16px;
            margin-top: 6px;
            font-size: 14px;
        }

        .hoslog-text {
            font-family: Noto Sans;
            font-size: 10px;
            font-weight: 400;
            line-height: 12px;
            letter-spacing: 0.5px;
            text-align: left;
            margin-bottom: 8px;
        }
