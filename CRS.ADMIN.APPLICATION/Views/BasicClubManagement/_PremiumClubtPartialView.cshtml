@model CRS.ADMIN.APPLICATION.Models.BasicClubManagement.ManagePremiumClubModel


<style>

    .landline-number-input {
        padding: 1px !important;
    }

    .box {
        display: flex;
        height: 40px;
        align-items: flex-start;
        gap: 10px;
        align-self: stretch;
    }

        .box i {
            display: flex;
            width: fit-content;
            height: 40px;
            padding: 0px 8px;
            align-items: center;
            gap: 5px;
            border-radius: 4px;
            border: 1px solid #A09E91;
        }

        .box p {
            color: #B7B7B7;
            font-family: &quot;
            Noto Sans&quot;
            font-size: 16px;
            font-style: normal;
            font-weight: 500;
            line-height: 100%;
        }
</style>

<div id="add-new-host-club" class="drawer-close-disable  flex justify-end fixed top-0 right-0 z-40 h-screen overflow-y-auto transition-transform translate-x-full "
     tabindex="-1" aria-labelledby="drawer-right-label">
    <div class="bg-white w-[863px] drawer-container relative">
        <div class="drawer-form-container sticky top-0 z-[5] ">
            <div class="drawer-form flex justify-between bg-secondary-color-pink py-[16px] px-[24px] h-[56px]">
                <div>  @CRS.ADMIN.APPLICATION.Resources.Resource.Manage_Club_Profile</div>
                <button type="button" data-drawer-hide="add-new-host-club" aria-controls="add-new-host-club" onclick="CancelFunction()"
                        class="text-[20px]">
                    <i class="las la-window-close text-white"></i>
                </button>
            </div>
        </div>

